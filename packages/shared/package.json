{
  "name": "@moltpoker/shared",
  "version": "0.1.0",
  "description": "Shared types, schemas, and constants for MoltPoker",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./constants/*": {
      "types": "./dist/constants/*.d.ts",
      "import": "./dist/constants/*.js"
    },
    "./schemas/*": {
      "types": "./dist/schemas/*.d.ts",
      "import": "./dist/schemas/*.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
