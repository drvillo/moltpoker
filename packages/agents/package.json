{
  "name": "@moltpoker/agents",
  "version": "0.1.0",
  "description": "Reference poker agents for MoltPoker",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "molt-agent": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "dev:agent": "tsx src/cli.ts",
    "agent": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.37",
    "@ai-sdk/openai": "^3.0.25",
    "@jmespath-community/jmespath": "^1.3.0",
    "@moltpoker/poker": "workspace:*",
    "@moltpoker/sdk": "workspace:*",
    "@moltpoker/shared": "workspace:*",
    "ai": "^6.0.73",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "gray-matter": "^4.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
